<resources>

    <!-- Base application theme. -->
    <style name="Start" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--<item name="android:background">@drawable/roll_3</item>-->
        <!--<item name="android:windowBackground">@drawable/launch_screen</item>-->
    </style>
    <style name="Started" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">?android:windowNoTitle</item>

        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@drawable/roll_1</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
    <!--<item name="android:background">@drawable/roll_3</item>-->
    </style>

</resources>
